10 CLS
20 PRINT " MASUKKAN JUMLAH DATA :";
30 INPUT " ", DAT
40 FOR I = 1 TO DAT
    50 CLS
    60 PRINT TAB(3); "INPUT DATA"; I; TAB(17); "/"; DAT
    70 PRINT STRING$(22, "=")
    80 INPUT " NPM      : ", NPM$(I)
    90 INPUT " NAMA     : ", NAMA$(I)
    100 INPUT " KELAS    : ", KELAS$(I)
    110 INPUT " NILAI 1  : ", N1(I)
    120 INPUT " NILAI 2  : ", N2(I)
    130 IF N1(I) < N2(I) THEN GOTO 160
    140 MAX(I) = N1(I)
    150 GOTO 170
    160 MAX(I) = N2(I)
    170 LET TOTAL = TOTAL + MAX(I)
180 NEXT I
190 CLS
200 PRINT TAB(23); "DAFTAR NILAI MAHASISWA"
210 PRINT STRING$(66, "-")
220 PRINT " NO"; TAB(8); "NPM"; TAB(18); "NAMA"; TAB(26); "KELAS"; TAB(33); "NILAI 1"; TAB(42); "NILAI 2"; TAB(51); "NILAI TERTINGGI"
230 PRINT STRING$(66, "-")
240 FOR J = 1 TO DAT
    250 PRINT J; TAB(6); NPM$(J); TAB(16); NAMA$(J); TAB(26); KELAS$(J); TAB(34); N1(J); TAB(43); N2(J); TAB(56); MAX(J)
260 NEXT J
270 PRINT STRING$(66, "-")
280 PRINT TAB(38); "TOTAL NILAI :"; TAB(56); TOTAL
290 END
