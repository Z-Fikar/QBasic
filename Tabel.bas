10 CLS
20 FOR I = 1 TO 10
    30 PRINT TAB(3); "INPUT DATA KE -"; I
    40 PRINT STRING$(21, "=")
    50 INPUT " NPM      : ", NPM$(I)
    60 INPUT " NAMA     : ", NAMA$(I)
    70 INPUT " KELAS    : ", KELAS$(I)
    80 INPUT " NILAI 1  : ", N1(I)
    90 INPUT " NILAI 2  : ", N2(I)
    100 INPUT " NILAI 3  : ", N3(I)
    110 LET RATA(I) = (N1(I) + N2(I) + N3(I)) / 3
    120 LET TOTAL = TOTAL + RATA(I)
    130 PRINT
140 NEXT I
150 CLS
160 PRINT TAB(29); "DAFTAR NILAI SISWA"
170 PRINT STRING$(69, "-")
180 PRINT " NO"; TAB(8); "NPM"; TAB(18); "NAMA"; TAB(26); "KELAS"; TAB(33); "NILAI 1"; TAB(42); "NILAI 2"; TAB(51); "NILAI 3"; TAB(60); "RATA-RATA"
190 PRINT STRING$(69, "-")
200 FOR J = 1 TO 1
    210 PRINT J; TAB(6); NPM$(J); TAB(16); NAMA$(J); TAB(26); KELAS$(J); TAB(34); N1(J); TAB(43); N2(J); TAB(52); N3(J); TAB(62); RATA(J)
220 NEXT J
230 PRINT STRING$(69, "-")
240 PRINT TAB(47); "TOTAL NILAI :"; TAB(62); TOTAL
250 END
